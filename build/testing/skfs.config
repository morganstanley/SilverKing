
          mntDir="skfs_mnt"
  silverkingBase=/var/tmp/silverking
        skfsBase=$silverkingBase/skfs
       skfsMount=$skfsBase/$mntDir
        skfsLogs=$skfsBase/logs
       checkFile=$skfsMount/intex/test.txt
fsNativeOnlyFile=$skfsBase/fsNativeOnlyPaths;

initialHeapSize=10
maxHeapSize=8192
heapDumpBase=/tmp/silverking
jvmOptions="-Xms${initialHeapSize}M,-Xmx${maxHeapSize}M,-XX:+HeapDumpOnOutOfMemoryError,-XX:HeapDumpPath=${heapDumpBase}/${GCName}.heap.dump"

     fuseLib=
     fuseBin=
   fuseLibKO=/lib/modules/$(uname -r)/kernel/fs/fuse
useBigWrites=
